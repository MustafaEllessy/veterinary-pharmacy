  <Code> Public Function ArNumber(ByVal str As String) As String
       Dim s As String = ""
       Dim ch() As Char = str.ToCharArray

       For Each chr As Char In ch
           Select Case chr
               Case "1"
                   chr = "١"
                   s += chr
               Case "2"
                   chr = "٢"
                   s += chr
               Case "3"
                   chr = "٣"
                   s += chr
               Case "4"
                   chr = "٤"
                   s += chr
               Case "5"
                   chr = "٥"
                   s += chr
               Case "6"
                   chr = "٦"
                   s += chr
               Case "7"
                   chr = "٧"
                   s += chr
               Case "8"
                   chr = "٨"
                   s += chr
               Case "9"
                   chr = "٩"
                   s += chr
               Case "0"
                   chr = "٠"
                   s += chr
               Case Else
                   s += chr

           End Select

       Next
       Return s
   End Function
</Code>
